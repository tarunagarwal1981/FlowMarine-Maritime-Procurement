{
  "name": "@flowmarine/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:comprehensive": "vitest run src/test/comprehensive.e2e.test.ts --reporter=verbose",
    "test:task-20-2": "tsx src/test/validateTask20-2.ts && npm run test:comprehensive",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "prisma": "^5.1.1",
    "@prisma/client": "^5.1.1",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.4",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1",
    "zod": "^3.21.4",
    "socket.io": "^4.7.2",
    "axios": "^1.4.0",
    "cron": "^2.4.1",
    "uuid": "^9.0.0",
    "tesseract.js": "^4.1.1",
    "xlsx": "^0.18.5",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/nodemailer": "^6.4.9",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.2",
    "@types/node": "^20.4.5",
    "@types/pdfkit": "^0.12.9",
    "@types/pg": "^8.10.0",
    "tsx": "^3.12.7",
    "vitest": "^0.34.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^0.34.1"
  }
}